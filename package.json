{
    "name": "voby",
    "repository": "github:fabiospampinato/voby",
    "description": "A high-performance framework with fine-grained observable-based reactivity for building rich applications.",
    "version": "0.44.1",
    "type": "module",
    "sideEffects": false,
    "//main": "dist/index.js",
    "//types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./via": {
            "import": "./dist/via.js",
            "types": "./dist/via.d.ts"
        },
        "./ssr": {
            "import": "./dist/ssr.js",
            "types": "./dist/ssr.d.ts"
        },
        "./jsx-runtime": {
            "import": "./dist/jsx-runtime/index.js",
            "types": "./dist/jsx-runtime/index.d.ts"
        },
        "./jsx-dev-runtime": {
            "import": "./dist/jsx/jsx-runtime.js",
            "types": "./dist/jsx/jsx-runtime.d.ts"
        },
        "./via/jsx-runtime": {
            "import": "./dist/via/jsx-runtime/index.js",
            "types": "./dist/via/jsx-runtime/index.d.ts"
        },
        "./ssr/jsx-runtime": {
            "import": "./dist/ssr/jsx-runtime/index.js",
            "types": "./dist/ssr/jsx-runtime/index.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "jsx-runtime": [
                "./dist/jsx/runtime.d.ts"
            ],
            "jsx-dev-runtime": [
                "./dist/jsx/runtime.d.ts"
            ],
            "via/jsx-runtime": [
                "./dist/via/runtime.d.ts"
            ],
            "ssr/jsx-runtime": [
                "./dist/ssr/runtime.d.ts"
            ]
        }
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "clean": "tsex clean",
        "build": "tsc",
        "watch": "tsc --watch",
        "//dev": "vite --force --mode dev",
        "//prod": "vite --force",
        "//build": "vite build",
        "prepublishOnly": "pnpm run clean && pnpm run compile",
        "bump": "pnpm version patch"
    },
    "keywords": [
        "ui",
        "framework",
        "reactive",
        "observable",
        "fast",
        "performant",
        "performance",
        "small",
        "fine-grained",
        "updates"
    ],
    "dependencies": {
        "esbuild-ifdef": "^0.2.0",
        "htm": "^3.1.1",
        "oby": "workspace:*",
        "vhtml": "workspace:*",
        "via": "workspace:*"
    },
    "devDependencies": {
        "@types/node": "^18.13.0",
        "typescript": "^4.9.5",
        "vite": "^4.1.1",
        "vite-plugin-conditional-compiler": "^0.0.6"
    }
}